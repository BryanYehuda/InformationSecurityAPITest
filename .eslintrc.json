{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":0,
        "linebreak-style":0,
        "eol-last":0,
        "prefer-const":0,
        "no-param-reassign":0,
        "class-methods-use-this":0,
        "consistent-return":0
    }
}
